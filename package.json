{
    "name": "KawaiiBot02",
    "version": "1.0.0",
    "description": "A discord bot with voice commands, gifs/imgs",
    "main": "bot.js",
    "author": "Henry Vex Graves",
    "scripts": {
        "docs": "jsdoc -r bot.js ./source/ -R ./README.md -d ./documentation/",
        "test": "nyc --reporter=html --reporter=text mocha tests/*.js",
        "tests": "nyc --reporter=text mocha tests/*.js",
        "coverage": "nyc report --reporter=text-lcov | coveralls",
        "start": "node bot.js",
        "debug": "nodemon bot.js",
        "clean": "rm -r documentation && rm -r coverage && rm -r .nyc_output",
        "all": "npm run clean && npm run test && npm run docs && npm run coverage"
    },
    "dependencies": {
        "@discordjs/opus": "^0.4.0",
        "@google-cloud/text-to-speech": "^3.1.0",
        "axios": "^0.19.2",
        "bee-queue": "^1.2.3",
        "cheerio": "^1.0.0-rc.3",
        "cleverbot-node": "^0.3.11",
        "discord-player": "^2.3.1",
        "discord.io": "https://github.com/woor/discord.io/tarball/gateway_v6",
        "discord.js": "^12.2.0",
        "dotenv": "^8.2.0",
        "emoji-random": "^0.1.2",
        "giphy-api": "^2.0.1",
        "google-tts-api": "0.0.4",
        "image-downloader": "^4.0.1",
        "jsdoc": "^3.6.6",
        "mocha": "^8.3.1",
        "node-opus": "^0.3.3",
        "nodejs-web-scraper": "^3.1.4",
        "nodemon": "^2.0.7",
        "open": "^8.0.7",
        "pretty-error": "^3.0.3",
        "request": "^2.88.2",
        "say": "^0.16.0",
        "tenorjs": "^1.0.7",
        "url": "^0.11.0",
        "window": "^4.2.7",
        "winston": "^3.3.3",
        "youtube-mp3-downloader": "^0.7.3"
    },
    "devDependencies": {
        "coveralls": "^3.1.0",
        "mocha-lcov-reporter": "^1.3.0",
        "nyc": "^15.1.0"
    }
}
